{
  "name": "@gesslar/colours",
  "description": "256 colours for your terminal expressions. You're welcome.",
  "author": {
    "name": "gesslar",
    "email": "bmw@gesslar.dev",
    "url": "https://gesslar.dev"
  },
  "version": "0.7.1",
  "license": "Unlicense",
  "homepage": "https://github.com/gesslar/colours#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gesslar/colours.git"
  },
  "keywords": [
    "colours",
    "256",
    "curious",
    "george",
    "ansi",
    "ascii",
    "trifle",
    "monkeynado"
  ],
  "packageManager": "pnpm@10.27.0",
  "type": "module",
  "main": "src/Colours.js",
  "bin": "src/cli.js",
  "exports": {
    ".": {
      "types": "./types/Colours.d.ts",
      "default": "./src/Colours.js"
    }
  },
  "files": [
    "src/",
    "types/",
    "UNLICENSE.txt"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "lint": "eslint src/",
    "test": "node --test test/Colours.test.js",
    "test:visual": "node test/test.js",
    "test-colours": "node src/cli.js",
    "clean": "rm -rfv ./dist",
    "build": "pnpm run clean && mkdir -pv ./dist && pnpm pack --pack-destination ./dist/",
    "update": "pnpm up --latest --recursive",
    "submit": "pnpm publish --access public --//registry.npmjs.org/:_authToken=\"${NPM_ACCESS_TOKEN}\"",
    "pr": "gt submit -p --ai",
    "patch": "pnpm version patch",
    "minor": "pnpm version minor",
    "major": "pnpm version major",
    "types": "node -e \"require('fs').rmSync('types',{recursive:true,force:true});\" && tsc -p tsconfig.types.json"
  },
  "devDependencies": {
    "@gesslar/uglier": "^0.5.1",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
